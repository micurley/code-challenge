{
  "name": "code-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "node-sass": "^4.8.3",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.1"
  },
  "scripts": {
    "_symlink-env": "rm .env && ln -s ../dotenv/common .env",
    "start": "npm run _symlink-env && npm run watch-css && react-scripts start",
    "build": "npm run _symlink-env && npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
